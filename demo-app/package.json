  "scripts": {
    "start": "env-cmd -f .env.dev react-scripts start",
    "local": "PORT=3011 env-cmd -f .env.dev react-scripts start",
    "live": "PORT=4011 env-cmd -f .env.production react-scripts start",
    "build": "env-cmd -f .env.dev react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:live": "env-cmd -f .env.production react-scripts build",
    "stage": "NODE_ENV=stage forever start -c \"npm run dynamite_digital_task_portal_serve_stage\" ./",
    "dynamite_digital_task_portal_serve_stage": "live-server build/ --entry-file=index.html --port=4608",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
